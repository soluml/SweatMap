  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "mocha": "^6.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"

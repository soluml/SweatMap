  "version": "0.2.0",
  "author": {
    "name": "Benjamin Solum",
    "email": "benjamin@soluml.com",
    "url": "http://soluml.com"
  },
  "preferGlobal": true,
  "keywords": [
    "sweat",
    "sweatmap",
    "sweat map",
    "sweatmap javascript",
